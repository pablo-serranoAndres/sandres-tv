<div class="featured-project-list-container">
  <h1>Editar proyectos <%= title %></h1>
  <button id="btn-handle-features" onclick="saveFeaturedProjects()">
    Guardar
  </button>
  <ul class="featured-project-list" id="featured-project-list">
    <% projects.forEach(project => { %>
    <li>
      <span data-id="<%= project.id %>"><%=project.title %></span>
      <div class="featured-buttons">
        <div class="featured-buttons-item">
          <span>Destacado</span>
          <button
            class="featured-edit-project-btn"
            onclick="toggleClass(event)"
          >
            <% if (project.featured) { %>
            <i
              id="featured-icon-<%= project.id %>"
              class="fa-solid fa-star"
              style="color: white"
            ></i>
            <% } else {%>
            <i
              id="featured-icon-<%= project.id %>"
              class="fa-regular fa-star"
              style="color: white"
            ></i>
            <% } %>
          </button>
        </div>
      </div>
    </li>
    <% }) %>
  </ul>
</div>
